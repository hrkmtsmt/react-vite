{
  "name": "app-name",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:serve\" \"pnpm test:watch\" \"pnpm lint\" \"pnpm format\"",
    "ci": "pnpm install --immutable --immutable-cache --check-cache",
    "test": "vitest run --config ./vite.config.ts",
    "spell-check": "cspell '**'",
    "lint": "eslint --config eslint.config.js .",
    "format": "prettier --write . --config ./prettier.config.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "husky install",
    "dev:serve": "vite --host localhost",
    "dev:no-test": "concurrently \"pnpm dev:serve\"",
    "test:watch": "vitest watch",
    "test:related": "vitest related --run"
  },
  "lint-staged": {
    "src/**/*": "pnpm test:related",
    "src/**/*.{ts,tsx,d.ts}": [
      "bash -c tsc --noEmit",
      "pnpm lint"
    ],
    "**/*.{ts,tsx,d.ts,json,css,scss}": "pnpm format",
    "*": "pnpm spell-check"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.18.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.0.3",
    "@eslint/js": "9.4.0",
    "@testing-library/react": "14.1.0",
    "@types/node": "20.17.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vitejs/plugin-react": "4.1.1",
    "concurrently": "8.2.2",
    "cspell": "8.8.3",
    "eslint": "9.4.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-functional": "6.5.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "globals": "^15.8.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "15.2.5",
    "prettier": "3.3.0",
    "ts-node": "10.9.1",
    "typescript": "5.4.5",
    "typescript-eslint": "7.11.0",
    "vite": "5.1.0",
    "vite-plugin-checker": "0.6.2",
    "vitest": "0.34.6"
  },
  "volta": {
    "node": "20.18.0"
  }
}
