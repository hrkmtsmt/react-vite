{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn dev:serve\" \"yarn test:watch\"",
    "test": "vitest run --config ./vite.config.ts",
    "spell-check": "cspell '**'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "husky install",
    "dev:serve": "vite --host localhost",
    "dev:no-test": "concurrently \"yarn dev:serve\"",
    "prettier:format": "prettier --write . --config ./.prettierrc",
    "test:watch": "vitest watch",
    "test:related": "vitest related --run"
  },
  "lint-staged": {
    "src/**/*": "yarn test:related",
    "src/**/*.{ts,tsx,d.ts}": [
      "bash -c tsc --noEmit",
      "yarn eslint"
    ],
    "**/*.{ts,tsx,d.ts,json,css,scss}": "yarn prettier:format",
    "*": "yarn spell-check"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.18.0"
  },
  "devDependencies": {
    "@testing-library/react": "14.1.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@vitejs/plugin-react": "4.1.1",
    "concurrently": "8.2.2",
    "cspell": "8.0.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-strict-dependencies": "1.2.3",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-plugin-checker": "0.6.2",
    "vitest": "0.34.6"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "4.0.1"
  },
  "packageManager": "yarn@4.0.1"
}
